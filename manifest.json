{
  "name": "SpikeK Blocker",
  "description": "HidesSpikeK posts from RushCard.IO",
  "version": "0.0.1",
  "manifest_version": 2,
  "icons": {
    "16": "yr_logo_16.png",
    "32": "yr_logo_32.png",
    "48": "yr_logo_48.png",
    "128": "yr_logo_128.png"
  },
  "background": {
    "scripts": ["./background.js"]
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": ["tabs", "*://*/*", "storage", "notifications"],
  "web_accessible_resources": ["notification_bg.png", "popup_logo.png"]
}
